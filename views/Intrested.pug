extends layout
block content
  h1= title
  - var result = results
  div.container-fluid
     div.row
        each item,index in result
            div.col-sm-2(onclick=`buy(${index})`) subjectName: #{item["subjectName"]}  <br>  section: #{item["section"]} <br> rollnumber: #{item["rollnumber"]} 
  script.
      function buy(index){
         alert("you purchased item "+(index+1))
      }